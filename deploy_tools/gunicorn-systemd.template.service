[Unit]
Description=Gunicorn Server for SITENAME

[Service]
Restart=on-failure
User=root
WorkingDirectory=/home/focus-sh/sites/SITENAME/source
ExecStart=/home/focus-sh/sites/superlists-staging.ottg.eu/virtualenv/bin/gunicorn --bind unix:/run/SITENAME.socket superlists.wsgi:application
	
[Install]
WantedBy=multi-user.target